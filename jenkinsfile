pipeline{
	agent any
	
	stages{
		stage('Build'){
			steps{
				git 'https://github.com/PradeshCnx/mule4Test.git'
				bat "mvn -Dmaven.test.failure.ignore-true clean package"
			}
		}
		stage('Test'){
			steps{
				git 'https://github.com/PradeshCnx/mule4Test.git'
				bat "mvn -Dmaven.test.failure.ignore-true clean test"
			}
		}
		stage('Deploy'){
			steps{
				git 'https://github.com/PradeshCnx/mule4Test.git'
				bat "mvn -Dmaven.test.failure.ignore-true clean Deploy -DmuleDeploye"
			}
		}
	}

}
